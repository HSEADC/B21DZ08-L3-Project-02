<div id="<%= dom_id swap %>">
<%= image_tag swap.swap_image.url, width: 400 if swap.swap_image.url %>
  <p>
    <strong>Название:</strong>
    <%= swap.title %>
  </p>

  <p>
    <strong>Описание:</strong>
    <%= swap.description %>
  </p>
  <p>
  <strong>Автор: </strong>
  <%= swap.user.email %>
</p>

</div>
