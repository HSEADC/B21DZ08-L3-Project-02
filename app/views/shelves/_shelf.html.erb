<div class="S_BodyPadding">
  <div class="M_TitleWrapper">
    <div class="W_PlantProfileTitle">
  
      <div class="A_Title">
        <%= shelf.title %>
      </div>

      <div class="A_AddPadding A_RegularText">
        <%= shelf.description %>
      </div>
    
      <div class="C_ProfileCards">
        <% @plants.each do |plant| %>
          <%= link_to plant, class: 'M_SwapCard' do %>
            <%= render partial: 'plants/plantPreview', locals: { plant: plant } %>
          <% end %>
        <% end %>
      </div>

    </div>
  </div>
</div>

