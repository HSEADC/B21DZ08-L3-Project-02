<div class="S_BodyPadding">
  <div class="M_TitleWrapper">
  <div class="W_PlantProfileTitle">
 
    <div class="A_Title">
      <%= shelf.title %>
    </div>

  <p>
    <strong>Description:</strong>
    <%= shelf.description %>
  </p>


  
  
<ul>
  <% @plants.each do |plant| %>
    <li><%= plant.name %></li>
    <div style="width: 100px">
    <%= image_tag plant.plant_image.url, class: "a" %>
    </div>
  <% end %>
</ul>

</div>
</div>
</div>
</div>
