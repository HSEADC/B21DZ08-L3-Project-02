<div class="S_BodyPadding">
  <div class="M_TitleWrapper">
      <div class="A_Title">мои предложения
      </div>
      <div class="A_AddPadding A_RegularText">Создайте свои предложения, чтобы поменяться растениями с другими<br> пользователями (или просто подарить)</div>
  </div>
  <div class="W_WishList">

  <div class="A_NewButton">
    <%= link_to(new_admin_swap_path, class: '') do %>
      <%= image_tag("base/plus.svg", class: "Q_Plus") %>
      <div class="A_RegularText">Добавить предложение</div>
    <% end %>
  </div>

      <div class="M_ExchageSwapCards">
      <% @swaps.each do |swap| %>
        <% if swap.user_id == current_user.id %>
          <%= link_to [:admin, swap], class: 'M_MySwapCard' do %>
            <%= render partial: 'admin/swaps/swapMyPreview', locals: { swap: swap } %>
          <% end %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>